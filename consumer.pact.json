{
  "consumer": {
    "name": "Thingies Consumer Example"
  },
  "provider": {
    "name": "Thingies Provider Example"
  },
  "interactions": [
    {
      "description": "get one thingy",
      "providerState": "has one thingy with '123' as an thingyId",
      "request": {
        "method": "GET",
        "path": "/thingies/123"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "id": "123",
          "name": "stuff"
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}
